{
    "expo": {
        "name": "BeSafe",
        "slug": "BeSafe",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./img/logo.png",
        "splash": {
            "image": "./img/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#0085FF"
        },
        "notification": {
            "icon": "./img/logo.png",
            "color": "#130e5c"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./img/logo.png",
                "backgroundColor": "#0085FF"
            },
            "package": "com.besafe"
        },
        "web": {
            "favicon": "./img/logo.png"
        },
        "plugins": [
            [
                "expo-notifications",
                {
                    "icon": "./img/logo.png",
                    "color": "#130e5c",
                    "sounds": ["./sound/noti.wav"]
                }
            ]
        ]
    }
}
