{
    "expo": {
        "name": "BeSafe",
        "slug": "BeSafe",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./img/logo.png",
        "splash": {
            "image": "./img/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#0085FF"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./img/logo.png",
                "backgroundColor": "#0085FF"
            }
        },
        "web": {
            "favicon": "./img/logo.png"
        }
    }
}
